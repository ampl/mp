model josephy.mod;
data josephy.dat;

set initpoint := 1..8;
param xinit {Rn,initpoint} >= 0;

data;
param xinit
:	1	2	3	4	5	6	7	8	:=
1	0	1	100	1	1	0	0	1.25
2	0	1	100	0	0	1	1	0
3	0	1	100	1	0	1	0	0
4	0	1	100	0	0	0	1	0.5 ;

for {point in initpoint}
{
	let{i in Rn} x[i] := xinit[i,point];
	solve;
	include compchk
}
