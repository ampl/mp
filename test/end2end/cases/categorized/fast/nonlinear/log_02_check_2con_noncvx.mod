# -------------------------------------------------------------
# log_02_check_2con_noncvx.mod.
# -------------------------------------------------------------

var x >=-100, <=100;
var y >=-100, <=100;

minimize Obj1:
       y;

subj to C1:
       y >= log(x);

subj to C2:
       2*y <= x;

subj to C3:
       x+y >= 1.2;
